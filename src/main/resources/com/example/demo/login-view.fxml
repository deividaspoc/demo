<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.LoginController">
   <children>
      <GridPane prefHeight="400.0" prefWidth="600.0" styleClass="root" stylesheets="@style.css">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Pane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Label layoutX="85.0" layoutY="136.0" text="Prisijungimo vardas:" />
                  <Label layoutX="85.0" layoutY="170.0" text="Slaptazodis:" />
                  <TextField fx:id="username" layoutX="196.0" layoutY="134.0" />
                  <PasswordField fx:id="password" layoutX="196.0" layoutY="167.0" />
                  <Button fx:id="login" layoutX="85.0" layoutY="201.0" mnemonicParsing="false" onAction="#onLoginButtonClick" text="Prisijungti" />
                  <Label fx:id="loginStatus" layoutX="150.0" layoutY="250.0" />
               </children></Pane>
         </children>
      </GridPane>
   </children>
</AnchorPane>
